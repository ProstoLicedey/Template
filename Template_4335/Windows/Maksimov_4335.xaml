<Window x:Class="Template_4335.Windows.Maksimov_4335"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template_4335.Windows"
        mc:Ignorable="d"
        Title="Maksimov_4335" Height="450" Width="800">
    <StackPanel>
        <TextBlock Text="Изменения выполнил студент группы 4335
                   Максимов Михаил"
               HorizontalAlignment="Center" VerticalAlignment="Center"
               FontSize="35" TextWrapping="Wrap" FontStyle="Oblique"/>
        <StackPanel Grid.Row="3" Orientation="Horizontal" HorizontalAlignment="Center" Margin="10">
            <Button x:Name="ExportForExcelButton" Margin="10" Height="40"  Width="100" Click="ExportForExcelButton_OnClick">Экспорт в Excel</Button>
            <Button x:Name="ImportFromExcelButton" Margin="10" Width="150" Click="ImportButton_OnClick">Импорт данных из Excel-файла</Button>
            <Button x:Name="ImportFromJson" Margin="10" Width="150" Click="ImportFromJson_OnClick">Импорт данных из Json-файла</Button>
            <Button x:Name="ExportToWordButton" Margin="10" Width="150" Click="ExportToWordButton_OnClick">Экспорт данных в Word</Button>
        </StackPanel>
    </StackPanel>
</Window>
